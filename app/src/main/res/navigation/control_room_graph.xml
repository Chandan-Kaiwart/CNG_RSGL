<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/control_room_graph"
    app:startDestination="@id/controlRoomLcvFrag">

    <fragment
        android:id="@+id/controlRoomLcvFrag"
        android:name="com.apc.cng_hpcl.home.controlRoom.ControlRoomLcvFrag"
        android:label="Control Room">
        <action
            android:id="@+id/action_controlRoomLcvFrag_to_controlRoomPhotoFrag"
            app:destination="@id/controlRoomPhotoFrag" />
        <argument
            android:name="type"
            app:argType="integer" />
        <argument
            android:name="station"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="lcv"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="latlng"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/controlRoomPhotoFrag"
        android:name="com.apc.cng_hpcl.home.controlRoom.ControlRoomPhotoFrag"
        android:label="ControlRoomPhotoFrag" >
        <argument
            android:name="type"
            app:argType="integer" />
        <action
            android:id="@+id/action_controlRoomPhotoFrag_to_controlRoomLcvFrag"
            app:destination="@id/controlRoomLcvFrag" />
    </fragment>
</navigation>